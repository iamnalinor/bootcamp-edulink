msgstr "Nome de arquivo inválido."

#: app/dialogs/containers.py:126
msgid ""
"Обработка...\n"
"\n"
"Это займет до 30 секунд."
msgstr ""
"Processando...\n"
"\n"
"Isso levará até 30 segundos."

#: app/dialogs/containers.py:151
#, python-brace-format
msgid "Отправлено! ID решения — <code>{homework.id}</code>"
msgstr "Enviado! ID da solução — <code>{homework.id}</code>"

#: app/dialogs/containers.py:160
msgid ":package: <b>Доступные контейнеры</b>"
msgstr ":package: <b>Contêineres disponíveis</b>"

#: app/dialogs/containers.py:189
#, python-brace-format, python-format
msgid ""
"\n"
":package: <b>Контейнер {{ container.name }}</b> › Просмотр\n"
"{% if container.is_archived %}:lock: Архивирован\n"
"{% elif container.deadline %}Дедлайн <b>{{ "
"container.deadline.strftime('%d.%m.%Y') }} 23:59 МСК</b>\n"
"{% endif %}\n"
"\n"
"{% if container.description %}<blockquote expandable>{{ "
"container.description }}</blockquote>\n"
"\n"
"{% endif %}\n"
"{% if homework_sent %}:white_check_mark: Отправлено решение под ID "
"<code>{{ homework_sent.id }}</code>\n"
"{% endif %}\n"
msgstr ""
"\n"
":package: <b>Contêiner {{ container.name }}</b> › Visualização\n"
"{% if container.is_archived %}:lock: Arquivado\n"
"{% elif container.deadline %}Prazo final <b>{{ "
"container.deadline.strftime('%d.%m.%Y') }} 23:59 MSK</b>\n"
"{% endif %}\n"
"\n"
"{% if container.description %}<blockquote expandable>{{ "
"container.description }}</blockquote>\n"
"\n"
"{% endif %}\n"
"{% if homework_sent %}:white_check_mark: Solução enviada com ID "
"<code>{{ homework_sent.id }}</code>\n"
"{% endif %}\n"

