msgstr "Mark"

#: app/dialogs/homeworks.py:181
msgid "Незачёт"
msgstr "Not pass"

#: app/dialogs/homeworks.py:221
msgid ":package: <b>Доступные задания</b>"
msgstr ":package: <b>Available assignments</b>"

#: app/dialogs/homeworks.py:223
msgid ":inbox_tray: Скачать файлы"
msgstr ":inbox_tray: Download files"

#: app/dialogs/homeworks.py:228
msgid ":hash: Скачать таблицу"
msgstr ":hash: Download as table"

#: app/dialogs/homeworks.py:253
#, python-format, python-brace-format
msgid ""
"\n"
":package: <b>Контейнер {{ container.name }} › Решение {{ homework.id }}</b>\n"
"Отправил {{ homework.owner.fio }} в {{ created_at }}\n"
"\n"
"{% if homework.text %}\n"
"<blockquote expandable>{{ homework.text }}</blockquote>\n"
"\n"
"{% endif %}\n"
"{% if homework.mark == -1 %}\n"
":no_entry: <b>Выставлен незачёт</b>\n"
"{% elif homework.mark %}\n"
":hash: <b>Выставлена оценка {{ homework.mark }}</b>\n"
"{% endif %}\n"
msgstr ""
"\n"
":package: Container {{ container.name }} › Solution {{ homework.id }}\n"
"Sent by {{ homework.owner.fio }} at {{ created_at }}\n"
"\n"
"{% if homework.text %}\n"
"<blockquote expandable>{{ homework.text }}</blockquote>\n"
"\n"
"{% endif %}\n"
"{% if homework.mark == -1 %}\n"
":no_entry: <b>Marked as not passed</b>\n"
"{% elif homework.mark %}\n"
":hash: <b>Mark: {{ homework.mark }}</b>\n"
"{% endif %}\n"

#: app/dialogs/homeworks.py:276
msgid ":hash: Выставить оценку"
msgstr ":hash: Add mark"

